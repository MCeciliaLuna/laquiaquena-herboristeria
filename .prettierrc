{
  "singleQuote": true,
  "trailingComma": "all",
  "tabWidth": 2,
  "endOfLine": "auto",
  "importOrder": [
    "^[./]",
    "<THIRD_PARTY_MODULES>",
    "^@assets/(.*)$",
    "^@components/(.*)$",
    "^@configs/(.*)$",
    "^@constants/(.*)$",
    "^@locales/(.*)$",
    "^@mock/(.*)$",
    "^@services/(.*)$",
    "^@store/(.*)$",
    "^@utils/(.*)$",
    "^@views/(.*)$",
    "^@/(.*)$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
